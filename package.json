{
	"name": "demo-plugin",
	"author": "Amit Dudhat",
	"version": "1.0.0",
	"description": "demo plugin for WordPress.",
	"private": true,
	"scripts": {
		"init": "./bin/init.js",
		"prod": "cross-env NODE_ENV=production webpack --mode production --progress",
		"dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
		"clean": "rm -rf build/*",
		"lint-css": "stylelint admin/sass/**/*.scss --syntax scss && stylelint public/sass/**/*.scss --syntax scss ",
		"lint-js": "eslint admin/js/ && eslint public/js/ ",
		"pot": "wp-pot --src '**/*.php' --dest-file 'languages/demo-plugin.pot' --domain 'demo-plugin' --package 'Demo Plugin' ",
		"precommit": "npm run lint-css && npm run lint-js && npm run pot"
	},
	"dependencies": {
		"webfontloader": "^1.6.28"
	},
	"peerDependencies": {
		"postcss": "^8.2.3"
	},
	"devDependencies": {
		"@babel/core": "^7.8.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0",
		"@babel/plugin-transform-async-to-generator": "^7.1.0",
		"@babel/plugin-transform-react-jsx": "^7.1.6",
		"@babel/preset-env": "^7.12.1",
		"autoprefixer": "^9.7.4",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "^7.0.0",
		"css-loader": "^3.4.2",
		"css-mqpacker": "^7.0.0",
		"cssnano": "^4.1.10",
		"eslint": "^6.8.0",
		"eslint-config-wordpress": "^2.0.0",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-react": "^7.11.1",
		"file-loader": "^5.1.0",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.14.1",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"postcss-assets": "^5.0.0",
		"postcss-loader": "^3.0.0",
		"postcss-pxtorem": "^5.0.0",
		"postcss-scss": "^3.0.4",
		"postcss-sort-media-queries": "^3.0.0",
		"prompt-sync": "^4.2.0",
		"replace-in-file": "^5.0.2",
		"sass-loader": "^8.0.2",
		"serialize-javascript": "^3.1.0",
		"style-loader": "^1.1.3",
		"stylelint": "^13.2.0",
		"stylelint-config-wordpress": "^16.0.0",
		"stylelint-webpack-plugin": "^1.2.3",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^5.4.0",
		"webpack-cli": "^3.3.11",
		"wp-pot-cli": "^1.3.0"
	},
	"browserslist": [
		"defaults"
	]
}
